package javaapplication253;

public class JavaApplication253 {

    public static void main(String[] args) {
  
        
        Escola minhaEscola = new Escola("Colégio São lucas", "R. Vinte e Cinco de Julho - vargas, 564");

     
        System.out.println("Nome da escola: " + minhaEscola.getNome());
        System.out.println("Endereço da escola: " + minhaEscola.getEndereco());

        minhaEscola.setNome("São lucas");
        minhaEscola.setEndereco("Rua vinte e cinco de julho 564 - vargas ");

        System.out.println("\nApós atualização:");
        System.out.println("Nome da escola: " + minhaEscola.getNome());
        System.out.println("Endereço da escola: " + minhaEscola.getEndereco());
    }
}
